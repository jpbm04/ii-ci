#Die DetailsURL wird in der Update-Collection geführt, die DownloadURL verweisst direkt auf das ZIP-File der Erweiterung
export ci_ext_detailsurl="http://localhost:8080/job/JoomlaCI/lastSuccessfulBuild/artifact/updates/"
export ci_ext_downloadurl="http://localhost:8080/job/JoomlaCI/lastSuccessfulBuild/artifact/zips/"

#Zu verwendender Codingstandard
#export ci_coding_standard="PSR2"
export ci_coding_standard="build/coding-standards/Joomla"

#Zu ignorierende Dateien für die Metrikerstellung
export ci_metrics_ignore="tests/,PHPExcel/,fpdf/,ICalender/,iCalcreator*/,nusoap/,lib_fpdf/,lib_icalcreator/,lib_phpexcel/,lib_jquery/,lib_nusoap/,helper/lib/,coding_standards/"
